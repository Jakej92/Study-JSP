@Override
public Result execute(HttpServletRequest req, HttpServletResponse resp) throws ServletException, IOException {
    System.out.println("ğŸš€ JoinOkController ì‹¤í–‰ë¨!");

    Result result = new Result();
    AdminDAO adminDAO = new AdminDAO();
    AdminVO adminVO = new AdminVO();
    HttpSession session = req.getSession();

    // âœ… ìš”ì²­ëœ íŒŒë¼ë¯¸í„° ê°€ì ¸ì˜¤ê¸°
    String adminAccount = req.getParameter("adminAccount");
    String adminPassword = req.getParameter("adminPassword");
    String adminName = req.getParameter("adminName");
    String adminPhone = req.getParameter("adminPhone");
    String adminEmail = req.getParameter("adminEmail");

    adminVO.setAdminAccount(adminAccount);
    adminVO.setAdminPassword(adminPassword);
    adminVO.setAdminName(adminName);
    adminVO.setAdminPhone(adminPhone);
    adminVO.setAdminEmail(adminEmail);

    // âœ… ì¤‘ë³µ ê³„ì • ê²€ì‚¬
    Optional<AdminVO> foundAdmin = adminDAO.selectByAdminAccount(adminAccount);
    if (foundAdmin.isPresent()) {
        System.out.println("âŒ ì¤‘ë³µëœ ê³„ì •ì´ ìˆìŠµë‹ˆë‹¤. íšŒì›ê°€ì… ë¶ˆê°€.");
        result.setPath("/join.admin?account=false");  
        result.setForward(false); // âœ… ğŸ”¥ ë°˜ë“œì‹œ Redirect ì‚¬ìš©!
        return result;
    }

    // âœ… ì¤‘ë³µëœ ê³„ì •ì´ ì—†ì„ ë•Œë§Œ íšŒì›ê°€ì… ì§„í–‰
    adminDAO.insert(adminVO);
    System.out.println("âœ… íšŒì›ê°€ì… ì„±ê³µ! ê´€ë¦¬ì ê³„ì • ì¶”ê°€ë¨.");

    session.setAttribute("admin", adminVO);

    // âœ… íšŒì›ê°€ì… í›„ `/login.admin`ìœ¼ë¡œ ì´ë™í•˜ë„ë¡ ìˆ˜ì •!
    result.setPath("/login.admin");  // ğŸ”¥ íšŒì›ê°€ì… ì„±ê³µ ì‹œ ë¡œê·¸ì¸ í˜ì´ì§€ë¡œ ì´ë™
    result.setForward(false);  // ğŸ”¥ ë°˜ë“œì‹œ Redirect ì‚¬ìš©!
    return result;
}
